<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

    <bean id = "OutputHelper" class="loosely_coupled.OutputHelper">
        <property name="outputGenerator" ref="JsonOutputGenerator" />

    </bean>

    <bean id = "CsvOutputGenerator" class="loosely_coupled.CsvOutputGenerator" />
    <bean id = "JsonOutputGenerator" class="loosely_coupled.JosnOutputGenerator" />

</beans>